{
  "name": "riot-typed",
  "version": "1.0.2",
  "description": "a tiny wrapper layer for riotjs to write riot tags with benefits of typescipt",
  "keywords": [
    "riot-typed",
    "typed",
    "typescript",
    "riot",
    "riotjs",
    "riot.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joylei/riot-typed.git"
  },
  "author": "joylei <leingliu@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run clean && npm run dist && npm run build:example && echo open in browser: ./examples/index.html",
    "build:example": "tsc -p ./examples/",
    "dist": "tsc -p ./",
    "clean": "shx rm -rf dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "riot": "^3.6.3",
    "shx": "^0.2.2",
    "typescript": "^2.4.2"
  }
}
