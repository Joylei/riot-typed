{
  "name": "riot-typed",
  "version": "0.1.1",
  "description": "a tiny wrapper layer for riotjs to write riot tags with benefits of typescipt",
  "keywords": [
    "riot-typed",
    "typed",
    "typescript",
    "riot",
    "riotjs",
    "riot.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joylei/riot-typed.git"
  },
  "author": "joylei <leingliu@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run clean && npm run dist && npm run build:example && open ./examples/index.html",
    "build:example": "node node_modules/.bin/tsc -p ./examples/",
    "dist": "node node_modules/.bin/tsc -p ./",
    "clean": "rm -f -r dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },

  "devDependencies": {
    "riot": "^2.3.12",
    "typescript": "^1.8.7"
  }
}
